(eval-when-compile (require 'color-theme))

(defun color-theme-birds_of_paradise ()
	(interactive)
	(color-theme-install '(color-theme-birds_of_paradise 
	(
		(foreground-color . "#E6E1C4")
		(background-color . "#372725")
	)
	(font-lock-string-face ((t (:foreground "#D9D762"))))
	(font-lock-type-face ((t (:foreground "#EFAC32"))))
	(font-lock-comment-face ((t (:foreground "#6B4E32"))))
	(font-lock-variable-name-face ((t (:foreground "#E6E1C4"))))
	(font-lock-comment-delimiter-face ((t (:foreground "#6B4E32"))))
	(font-lock-doc-face ((t (:foreground "#6B4E32"))))
	(font-lock-keyword-face ((t (:foreground "#EF5D32"))))
	(font-lock-constant-face ((t (:foreground "#6C99BB"))))
	(font-lock-preprocessor-face ((t (:foreground "#EF5D32"))))
	(font-lock-doc-string-face ((t (:foreground "#6B4E32"))))
	(font-lock-builtin-face ((t (:foreground "#EFAC32"))))
	(font-lock-function-name-face ((t (:foreground "#EFAC32"))))
)))
